w MarkdownIt();\r\n\t\t\t\tconst result = md.render(blog.content);\r\n\t\t\t\t_this.blog.content = result\r\n\r\n\t\t\t\t//若当前博客页面的用户ID等于当前登录用户的ID时,则展示'编辑'及'删除'按钮\r\n\t\t\t\t_this.islogin = (blog.userId === _this.$store.getters.getUserInfo.id)\r\n\t\t\t})\r\n\t\t}\r\n\t}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/*文章内容页面样式*/\r\n.m-blog {\r\n\tbox-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n\twidth: 1100px;\r\n\tmin-height: 600px;\r\n\tmargin: 0 auto;\r\n\tpadding: 20px 40px;\r\n}\r\n\r\n/*文章标题样式*/\r\n.m-header-title {\r\n\ttext-align: center;\r\n\tfont-size: 30px;\r\n\tfont-family: 'Dubai Light';\r\n\tpadding-top: 25px;\r\n}\r\n\r\n/*文章内容样式*/\r\n.markdown-body {\r\n\tmargin-top: 120px;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}